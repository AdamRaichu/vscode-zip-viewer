{
  "name": "zip-viewer",
  "displayName": "Zip Tools",
  "description": "%extension.description%",
  "publisher": "adamraichu",
  "icon": "logo.png",
  "version": "3.2.0",
  "license": "MIT",
  "author": {
    "name": "AdamRaichu"
  },
  "categories": [
    "Visualization"
  ],
  "keywords": [
    "zip",
    "extract",
    "jar",
    "preview",
    "viewer",
    "gzip"
  ],
  "repository": {
    "url": "https://github.com/AdamRaichu/vscode-zip-viewer"
  },
  "main": "./dist/main.js",
  "browser": "./dist/main.js",
  "activationEvents": [
    "onCommand:zipViewer.extract",
    "onCommand:zipViewer.zip",
    "onCommand:zipViewer.openFileWithEditor",
    "onCustomEditor:zipViewer.ZipEdit",
    "onCustomEditor:zipViewer.GZipEdit"
  ],
  "engines": {
    "vscode": "^1.63.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "zipViewer.extract",
        "title": "%command.extract%",
        "shortTitle": "%command.extract.short%"
      },
      {
        "command": "zipViewer.zip",
        "title": "%command.zip%",
        "shortTitle": "%command.zip.short%"
      },
      {
        "command": "zipViewer.openFileWithEditor",
        "title": "%command.openFileWithEditor%",
        "shortTitle": "%command.openFileWithEditor.short%",
        "icon": "$(explorer-view-icon)"
      }
    ],
    "customEditors": [
      {
        "viewType": "zipViewer.ZipEdit",
        "displayName": "Zip Tools",
        "selector": [
          {
            "filenamePattern": "*.zip"
          },
          {
            "filenamePattern": "*.vsix"
          },
          {
            "filenamePattern": "*.mcworld"
          },
          {
            "filenamePattern": "*.mcpack"
          },
          {
            "filenamePattern": "*.mcaddon"
          },
          {
            "filenamePattern": "*.jar"
          },
          {
            "filenamePattern": "*.pbit"
          },
          {
            "filenamePattern": "*.pbix"
          },
          {
            "filenamePattern": "*.ipa"
          },
          {
            "filenamePattern": "*.xlsx"
          }
        ],
        "priority": "default"
      },
      {
        "viewType": "zipViewer.GZipEdit",
        "displayName": "Zip Tools",
        "selector": [
          {
            "filenamePattern": "*.gz"
          },
          {
            "filenamePattern": "*.gzip"
          },
          {
            "filenamePattern": "*.svgz"
          },
          {
            "filenamePattern": "*.emz"
          },
          {
            "filenamePattern": "*.tg"
          },
          {
            "filenamePattern": "*.tgz"
          }
        ],
        "priority": "default"
      }
    ],
    "configuration": [
      {
        "title": "Zip Tools",
        "properties": {
          "zipViewer.zipTypes": {
            "type": "array",
            "description": "%config.zipTypes.description%",
            "items": {
              "type": "string",
              "title": "%config.zipTypes.items.title%",
              "pattern": "\\.([A-Za-z0-9\\.])"
            },
            "default": [
              ".zip",
              ".vsix",
              ".mcworld",
              ".mcpack",
              ".mcaddon",
              ".jar",
              ".pbit",
              ".pbix",
              ".ipa",
              ".xlsx"
            ]
          },
          "zipViewer.picky": {
            "type": "boolean",
            "markdownDescription": "%config.picky.description%",
            "default": false
          },
          "zipViewer.unzippedSuffix": {
            "type": "string",
            "markdownDescription": "%config.unzippedSuffix.description%",
            "default": "_unzipped",
            "pattern": "^[^\\/?%*:|\"<>]+$"
          },
          "zipViewer.gzipEditorEnabled": {
            "type": "boolean",
            "markdownDescription": "%config.gzipEditorEnabled.description%",
            "default": true
          }
        }
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "zipViewer.zip",
          "when": "explorerResourceIsFolder",
          "group": "2_workspace"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/vscode": "^1.63.0",
    "@vscode/vsce": "^2.16.0",
    "gzip": "^0.1.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "pako": "^2.1.0"
  },
  "scripts": {
    "build": "webpack"
  }
}