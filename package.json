{
  "name": "zip-viewer",
  "displayName": "Zip Tools",
  "description": "An extension which allows for the manipulation of zip files.",
  "publisher": "adamraichu",
  "icon": "logo.png",
  "authors": "AdamRaichu",
  "author": {
    "name": "AdamRaichu"
  },
  "categories": [
    "Visualization"
  ],
  "keywords": [
    "zip",
    "extract"
  ],
  "repository": {
    "url": "https://github.com/AdamRaichu/vscode-zip-viewer"
  },
  "version": "2.0.0",
  "browser": "./dist/main.js",
  "main": "./dist/main.js",
  "activationEvents": [
    "onCommand:AdamRaichu.zipViewer.extract",
    "onCommand:AdamRaichu.zipViewer.zip",
    "onCustomEditor:zipViewer.ZipEdit"
  ],
  "engines": {
    "vscode": "^1.63.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "AdamRaichu.zipViewer.extract",
        "title": "Zip Viewer: Extract (unzip) a zip file from workspace",
        "shortTitle": "Extract Zip"
      },
      {
        "command": "AdamRaichu.zipViewer.zip",
        "title": "Zip Viewer: Create a zip file from folder",
        "shortTitle": "Zip Folder"
      }
    ],
    "customEditors": [
      {
        "viewType": "zipViewer.ZipEdit",
        "displayName": "Zip Viewer",
        "selector": [
          {
            "filenamePattern": "*.zip"
          },
          {
            "filenamePattern": "*.vsix"
          },
          {
            "filenamePattern": "*.mcworld"
          },
          {
            "filenamePattern": "*.mcpack"
          },
          {
            "filenamePattern": "*.mcaddon"
          }
        ],
        "priority": "default"
      }
    ],
    "configuration": [
      {
        "title": "Zip Extracter",
        "properties": {
          "zipViewer.zipTypes": {
            "type": "array",
            "description": "An array of file extensions to check files against when unzipping.",
            "items": {
              "type": "string",
              "title": "File Extension",
              "pattern": "\\.([A-Za-z0-9\\.])"
            },
            "default": [
              ".zip",
              ".vsix",
              ".mcworld",
              ".mcpack",
              ".mcaddon"
            ]
          },
          "zipViewer.picky": {
            "type": "boolean",
            "markdownDescription": "Whether the extension should automatically fail operations when file extension is not in `zipTypes`. If set to true, extension will only attempt to unzip files with extensions found in `zipTypes`.",
            "default": true
          },
          "zipViewer.unzippedSuffix": {
            "type": "string",
            "markdownDescription": "The suffix to add to folder name when unzipping a zip file. Must not contain any of the following characters: `/ ? % * : | \" < >`.",
            "default": "_unzipped",
            "pattern": "^[^\\/?%*:|\"<>]+$"
          }
        }
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.63.0"
  }
}