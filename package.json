{
  "name": "zip-viewer",
  "displayName": "Zip Extracter",
  "description": "An extension which allows for the extraction of zip files.",
  "publisher": "adamraichu",
  "icon": "logo.png",
  "authors": "AdamRaichu",
  "author": {
    "name": "AdamRaichu"
  },
  "categories": [
    "Visualization"
  ],
  "keywords": [
    "zip",
    "extract"
  ],
  "repository": {
    "url": "https://github.com/AdamRaichu/vscode-zip-viewer"
  },
  "version": "1.0.0",
  "browser": "/extension.js",
  "main": "/extension.js",
  "activationEvents": [
    "onCommand:AdamRaichu.zipViewer.extract"
  ],
  "engines": {
    "vscode": "^1.61.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "AdamRaichu.zipViewer.extract",
        "title": "Zip: Extract a zip file from workspace",
        "shortTitle": "Extract Zip"
      }
    ],
    "configuration": [
      {
        "title": "Zip Extracter",
        "properties": {
          "zipViewer.zipTypes": {
            "type": "array",
            "default": [],
            "description": "An additonal set of file extensions to check files against when extracting. Pattern: `.zip`"
          }
        }
      }
    ]
  }
}